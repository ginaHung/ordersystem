{
  "name": "typeorm-typescript-express",
  "version": "0.0.1",
  "readmeFilename": "README.md",
  "scripts": {
    "clean": "rimraf out/*",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "compile": "tsc",
    "copy-ssl": "copyfiles -f \"./ssl/*\" out/ssl",
    "start": "NODE_ENV=production node out/index.js",
    "dev:nowatch": "npm run lint && ts-node src/index.ts",
    "dev": "nodemon --watch src -e ts --exec npm run dev:nowatch",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config ./src/config/migration.ts",
    "migrate:up": "npm run typeorm migration:run",
    "migrate:down": "npm run typeorm migration:revert",
    "migrate:generate": "npm run typeorm migration:generate -- -n windwork"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.8",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.11.1",
    "@types/express-session": "^1.17.0",
    "@types/hapi__joi": "^16.0.12",
    "@types/helmet": "0.0.45",
    "@types/jest": "^24.0.25",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/multer": "^1.4.2",
    "@types/node": "^8.0.29",
    "@types/passport": "^1.0.3",
    "@types/swagger-jsdoc": "^3.0.2",
    "@types/swagger-ui-express": "^4.1.2",
    "copyfiles": "^2.2.0",
    "dotenv": "^8.2.0",
    "jest": "^24.9.0",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.3.0",
    "ts-node": "^3.3.0",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.1",
    "typescript": "3.3.3333"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "aws-sdk": "^2.654.0",
    "body-parser": "^1.18.2",
    "express": "^4.16.3",
    "express-session": "^1.17.0",
    "helmet": "^3.21.2",
    "http-status-codes": "^1.4.0",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^6.1.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "passport": "^0.4.1",
    "passport-azure-ad-oauth2": "0.0.4",
    "pg": "^7.17.0",
    "reflect-metadata": "^0.1.10",
    "swagger-jsdoc": "^4.0.0",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "0.2.22"
  }
}
